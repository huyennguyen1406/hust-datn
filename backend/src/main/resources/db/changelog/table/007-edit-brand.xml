<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="
          http://www.liquibase.org/xml/ns/dbchangelog
          https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.27.xsd">

    <changeSet id="007-edit-brand" author="huyennguyen1406" dbms="postgresql">
        <preConditions>
                <tableExists tableName="brand"/>
        </preConditions>
        <addColumn tableName="brand">
            <column name="brand_logo" type="nvarchar(2000)">
            </column>
            <column name="description" type="nvarchar(500)">
            </column>
        </addColumn>
    </changeSet>

</databaseChangeLog>
